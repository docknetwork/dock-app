{
  "name": "@docknetwork/wallet-monorepo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "mobile:watch": "yarn workspace @docknetwork/mobile-wallet watch",
    "mobile:run-ios": "yarn workspace @docknetwork/mobile-wallet run-ios",
    "mobile:run-android": "yarn workspace @docknetwork/mobile-wallet run-android",
    "mobile:pod-install": "cd ./packages/mobile/ios && pod update && pod install && cd -",
    "postinstall": "./node_modules/.bin/rn-nodeify --install --hack --yarn",
    "web:start": "yarn workspace @docknetwork/web-wallet start"
  },
  "_resolutions": {
    "jsonld": "3.2.0",
    "@polkadot/api": "4.10.1",
    "@polkadot/keyring": "6.4.1",
    "@polkadot/types": "4.10.1",
    "@polkadot/util": "6.4.1",
    "@polkadot/util-crypto": "6.4.1",
    "@polkadot/wasm-crypto": "^4.0.2"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": [
      "**react**",
      "**react-native**",
      "**native-base**"
    ]
  },
  "dependencies": {
    "@tradle/react-native-http": "^2.0.0",
    "browserify-zlib": "~0.1.4",
    "https-browserify": "~0.0.0",
    "native-base": "^3.0.7",
    "querystring-es3": "~0.2.0",
    "react-native-crypto": "^2.1.0",
    "react-native-level-fs": "^3.0.0",
    "react-native-os": "^1.0.1",
    "react-native-randombytes": "^3.0.0",
    "react-native-tcp": "^3.2.1",
    "react-native-udp": "^2.1.0",
    "readable-stream": "1.0.33",
    "string_decoder": "~0.10.25",
    "timers-browserify": "^1.0.1",
    "url": "~0.10.1",
    "util": "~0.10.3",
    "vm-browserify": "0.0.4"
  },
  "react-native": {
    "zlib": "browserify-zlib",
    "console": "console-browserify",
    "constants": "constants-browserify",
    "crypto": "react-native-crypto",
    "dns": "dns.js",
    "net": "react-native-tcp",
    "domain": "domain-browser",
    "http": "@tradle/react-native-http",
    "https": "https-browserify",
    "os": "react-native-os",
    "path": "path-browserify",
    "querystring": "querystring-es3",
    "fs": "react-native-level-fs",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "dgram": "react-native-udp",
    "stream": "stream-browserify",
    "timers": "timers-browserify",
    "tty": "tty-browserify",
    "vm": "vm-browserify",
    "tls": false
  },
  "browser": {
    "zlib": "browserify-zlib",
    "console": "console-browserify",
    "constants": "constants-browserify",
    "crypto": "react-native-crypto",
    "dns": "dns.js",
    "net": "react-native-tcp",
    "domain": "domain-browser",
    "http": "@tradle/react-native-http",
    "https": "https-browserify",
    "os": "react-native-os",
    "path": "path-browserify",
    "querystring": "querystring-es3",
    "fs": "react-native-level-fs",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "dgram": "react-native-udp",
    "stream": "stream-browserify",
    "timers": "timers-browserify",
    "tty": "tty-browserify",
    "vm": "vm-browserify",
    "tls": false
  }
}
